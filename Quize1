
package com.mycompany.quize1;
interface Mamal{
    void Goat();
    void Lion();
}
interface Bird{
    void Eagle();
}
 class animal implements Mamal, Bird{
     @Override
     public void Goat(){
         System.out.println("Goat eats grass");
     }@Override
     public void Lion(){
             System.out.println("Lion is King of animal");
     }
     @Override
     public void Eagle(){
         System.out.println("Eagle can flay");
     }
 }
public class Quize1 {

    public static void main(String[] args) {
        animal an= new animal();
        an.Goat();
        an.Lion();
        an.Eagle();
        System.out.println("this program is the concept of multiple interface.");
    }
}
